{
  "builds":[
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles",
        "command": "python manage.py collectstatic --noinput && gunicorn <your_project>.wsgi"
      }
    }
  ],
  
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
    
  ]
}
