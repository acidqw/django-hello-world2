
{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "gunicorn django_hello_world2.wsgi:application"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "django_hello_world2.settings"
  }
}